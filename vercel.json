{
  "version": 2,
  "installCommand": "npm install --prefix frontend --include=dev && npm install --prefix api",
  "buildCommand": "npm run build --prefix frontend",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
